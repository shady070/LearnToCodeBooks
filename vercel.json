{
  "rewrites": [
    {
      "source": "/wp-admin/:path*",
      "destination": "https://wp.learntocodebooks.com/wp-admin/:path*"
    },
    {
      "source": "/wp-login.php",
      "destination": "https://wp.learntocodebooks.com/wp-login.php"
    },
    {
      "source": "/wp-content/:path*",
      "destination": "https://wp.learntocodebooks.com/wp-content/:path*"
    },
    {
      "source": "/wp-includes/:path*",
      "destination": "https://wp.learntocodebooks.com/wp-includes/:path*"
    },
    {
      "source": "/create_autologin_:path*",
      "destination": "https://wp.learntocodebooks.com/create_autologin_:path*"
    }
  ]
}
